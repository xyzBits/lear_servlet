<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.google.learn.javabasic.thread.sxtdemo.net.httpserver.demo4.LoginServlet</servlet-class>
    </servlet>



    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-name>com.google.learn.javabasic.thread.sxtdemo.net.httpserver.demo4.RegisterServlet</servlet-name>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>

</web-app>
